/* Stomach acid is continually produced by the stomach and affects things within, wearing off rather quickly when contact is broken */
static effect stomach_acid {
    target internal;
    transform acid_burn;
    windup 1;
    duration 2;
    magnitude 0.5;
}

static effect delayed_death {
    target possessor;
    transform kill;
    windup 10;
    duration 0;
}
